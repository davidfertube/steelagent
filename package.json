{
  "name": "steelagent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest run tests/performance/",
    "test:bottleneck": "vitest run tests/performance/bottleneck-analysis.test.ts",
    "test:evaluation": "vitest run tests/evaluation/",
    "test:evaluation:unit": "vitest run tests/evaluation/ --testNamePattern='Unit|Pattern|Validation'",
    "test:evaluation:quick": "INTEGRATION_TEST=true vitest run tests/evaluation/baseline-vs-rag.test.ts --testNamePattern='quick|Integration'",
    "test:evaluation:full": "INTEGRATION_TEST=true vitest run tests/evaluation/",
    "test:evaluation:confusion": "INTEGRATION_TEST=true vitest run tests/evaluation/ --testNamePattern='Confusion'",
    "evaluation:report": "tsx scripts/generate-report.ts",
    "test:rag-metrics": "vitest run tests/evaluation/rag-metrics.test.ts",
    "evaluation:rag": "tsx scripts/run-rag-evaluation.ts",
    "evaluation:rag:verbose": "tsx scripts/run-rag-evaluation.ts --verbose",
    "test:accuracy": "tsx scripts/accuracy-test.ts",
    "test:accuracy:verbose": "tsx scripts/accuracy-test.ts --verbose",
    "test:mvp": "tsx scripts/mvp-accuracy-test.ts",
    "test:mvp:verbose": "tsx scripts/mvp-accuracy-test.ts --verbose",
    "test:confusion": "INTEGRATION_TEST=true vitest run tests/evaluation/ --testNamePattern='Confusion'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.12.41",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.1",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.29.0",
    "groq-sdk": "^0.37.0",
    "langfuse": "^3.38.6",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "unpdf": "^1.4.0",
    "voyageai": "^0.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "jsdom": "^24.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "5.9.3",
    "vitest": "^2.1.0"
  }
}
